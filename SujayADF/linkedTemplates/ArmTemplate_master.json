		},
		"SqlServerLS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "SUJAY-PC\\Sujay"
		},
		"SujayExcelLS_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\Sujay\\Desktop\\Azure\\Training\\"
